{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "customdatatable": [
        "dist/customdatatable"
      ],
      "customdatatable/*": [
        "dist/customdatatable/*"
      ],
      "ngdatagrid": [
        "dist/ngdatagrid"
      ],
      "ngdatagrid/*": [
        "dist/ngdatagrid/*"
      ],
      "ng-model-form": [
        "dist/ng-model-form"
      ],
      "ng-model-form/*": [
        "dist/ng-model-form/*"
      ],
      "ngmodelform": [
        "dist/ngmodelform"
      ],
      "ngmodelform/*": [
        "dist/ngmodelform/*"
      ],
      "ng-pagination": [
        "dist/ng-pagination"
      ],
      "ng-pagination/*": [
        "dist/ng-pagination/*"
      ],
      "ngpagination": [
        "dist/ngpagination"
      ],
      "ngpagination/*": [
        "dist/ngpagination/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}